<h1>Generate a random track</h1>
<hr/>

<% if @track.errors.any? %>
  <div>
    <strong>Some issues with your song</strong>
    <ul>
      <% @track.errors.full_messages.each do |track| %>
        <li><%= track %></li>
      <% end %>
    </ul>
  </div>
<% end %>

<form method="post" action="/tracks">
  <div>
    <input = type="submit" value="Submit">
  </div>
</form>